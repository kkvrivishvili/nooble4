# PostgreSQL Configuration
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DB=noobledb

# Kong Database Configuration
KONG_PG_USER=kong
KONG_PG_PASSWORD=kong
KONG_PG_DATABASE=kong

# Konga Configuration
KONGA_DATABASE_URI=postgresql://postgres:postgres@postgres:5432/konga

# Qdrant Configuration
QDRANT_URL=http://qdrant_database:6333

# Redis Configuration
REDIS_URL=redis://redis_database:6379/0

# Services URLs
AGENT_EXECUTION_SERVICE_URL=http://agent_execution_service:8005
QUERY_SERVICE_URL=http://query_service:8000
INGESTION_SERVICE_URL=http://ingestion_service:8002
CONVERSATION_SERVICE_URL=http://conversation_service:8004
EMBEDDING_SERVICE_URL=http://embedding_service:8006

# Keycloak Configuration
KEYCLOAK_URL=http://keycloak:8080
KEYCLOAK_REALM=nooble4
KEYCLOAK_CLIENT_ID=nooble4-client
KEYCLOAK_CLIENT_SECRET=your-client-secret

# JWT Secret
JWT_SECRET=your-jwt-secret-key-here

# Environment
NODE_ENV=development
