# Qdrant Database Configuration
QDRANT_HOST=0.0.0.0
QDRANT_HTTP_PORT=6333
QDRANT_GRPC_PORT=6334
QDRANT_API_KEY=
QDRANT_ALLOW_RECOVERY=true
QDRANT_STORAGE_PATH=/qdrant/storage

# Performance Tuning
QDRANT_STORAGE_OPTIMIZERS_CONFIG_DEFAULT_SEGMENT_NUMBER=2
QDRANT_STORAGE_OPTIMIZERS_CONFIG_MAX_OPTIMIZATION_THREADS=2

# Resource Limits
QDRANT_SERVICE_MAX_REQUEST_SIZE_MB=32

# Logging
QDRANT_LOG_LEVEL=INFO
